<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net40;net45;net46;net5.0;net6.0</TargetFrameworks>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Authors>gaoshang212</Authors>
		<LangVersion>10.0</LangVersion>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/gaoshang212/Gsof.System.Token</PackageProjectUrl>
		<RepositoryUrl>https://github.com/gaoshang212/Gsof.System.Token.git</RepositoryUrl>
		<AssemblyVersion>0.2.0</AssemblyVersion>
		<VersionPrefix>0.2.0</VersionPrefix>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Gsof" Version="0.3.1" />
		<PackageReference Include="Gsof.Native" Version="0.2.4" />
	</ItemGroup>

</Project>
